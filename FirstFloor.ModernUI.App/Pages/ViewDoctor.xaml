<UserControl x:Class="ModernUINavigationApp3.Pages.ViewDoctor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             Width="Auto"
             Height="Auto"
             d:DesignHeight="300"
             d:DesignWidth="300"
             mc:Ignorable="d">
    <Grid Style="{StaticResource ContentRoot}">
        <DataGrid x:Name="DataGrid2"
                  Margin="10,81,0,0"
                  HorizontalAlignment="Center" VerticalAlignment="Top" />
        <Button Content="Refresh" HorizontalAlignment="Left" Margin="99,34,0,0" VerticalAlignment="Top" Width="81" Click="Button_Click"/>
        <mui:ModernButton x:Name="delbutton" Content="Delete" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="99,108,0,0" Click="delbutton_Click"/>
    </Grid>
</UserControl>
